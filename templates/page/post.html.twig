{% extends 'base.html.twig' %}

{% block title %}Post
{% endblock %}

{% block body %}
	<div class="post-container">
		<div class=" d-flex flex-column justify-content-center align-items-center">
			<h4>{{post.title}}</h4>
			<img src={{post.image}} alt="">
		</div>
		<div>
			<h2>Description</h2>
			<p class=" text-start m-0">
				{{post.content}}
			</p>
		</div>
	</div>
{% endblock %}